{
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react",
    "import"
  ],
  "rules": {
    "react/prop-types": 0,
    "indent": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "double"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}